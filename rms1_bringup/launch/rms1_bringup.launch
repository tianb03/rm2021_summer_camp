    <!-- Run the static TF publisher from base to cam -->
    <node pkg="tf2_ros" type="static_transform_publisher" name="base_cam" args="0.05 -0.120 0.250 0.0 0.7854 0.0 base_link s1_camera_frame" />

    <!-- Run the tag detecting nodes -->
    <include file="$(find rms1_bringup)/launch/detecting.launch" />

    <!-- Run the planning nodes -->
    <include file="$(find rms1_bringup)/launch/planner.launch" />

    <!-- Run the odom calibration node -->
    <include file="$(find rms1_bringup)/launch/odom_calib.launch" />

    <!-- 在rviz中显示-->
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find rms1_bringup)/rviz/debug.rviz" required="true" />
    
</launch>
